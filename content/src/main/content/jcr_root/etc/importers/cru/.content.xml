<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="sling:OrderedFolder">
    <jcr:content
        jcr:primaryType="nt:unstructured"
        jcr:title="CRU"/>
	<givepageimport
	    jcr:primaryType="sling:Folder"
	    jcr:title="Give Pages Importer"
	    sling:resourceType="cq/ui/components/widget"
	    jsLibs="[cq.widgets]"
	    xtype="cruGiveDataImporter"
	    options="[
	    	/apps/cru-content-importer/configurations/give/staff,
	    	/apps/cru-content-importer/configurations/give/ministry,
	    	/apps/cru-content-importer/configurations/give/fund
	    ]"
	    tempStorePath="/var/cru-content-importer"
	    />
	<giveimagesimport
	    jcr:primaryType="sling:Folder"
	    jcr:title="Give Images Importer"
	    sling:resourceType="cq/ui/components/widget"
	    jsLibs="[cq.widgets]"
	    xtype="cruGiveDataImporter"
	    options="[
	    	/apps/cru-content-importer/configurations/give/staffImage,
	    	/apps/cru-content-importer/configurations/give/ministryImage,
	    	/apps/cru-content-importer/configurations/give/fundImage
	    ]"
	    tempStorePath="/var/cru-content-importer"
	    />
	<staffwebpageimport
	    jcr:primaryType="sling:Folder"
	    jcr:title="Staff Web Pages Importer"
	    sling:resourceType="cq/ui/components/widget"
	    jsLibs="[cq.widgets]"
	    xtype="cruGiveDataImporter"
	    options="[
	    	/apps/cru-content-importer/configurations/staffweb/article,
	    	/apps/cru-content-importer/configurations/staffweb/handbook,
	    	/apps/cru-content-importer/configurations/staffweb/landingpage
	    ]"
	    tempStorePath="/var/cru-content-importer"
	    />
</jcr:root>
