<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
	jcr:primaryType="sling:OrderedFolder">
	
	<article
		jcr:primaryType="nt:unstructured"
		title="Article"
		defaultPath="/content/staffweb/us/en"
		pageTemplate="/apps/StaffWeb/components/page/article"
		intermediateTemplate="/apps/StaffWeb/components/page/content"
		transformation="/apps/cru-content-importer/transformations/staffweb/article.xsl"
		rowColumnNames="{Long}0"
		columnFileName="primaryFile"
		columnMimeType=""
		pathCreationStrategy="[finalUrl:(.*)(?&lt;!\\/index)(\\/index)?\\..*:/$1]"
		pageAcceptRule=""
		factoryType="page"
		additionalMappingFile="{Boolean}true"
		additionalMappingLabel="Nodelink Mapping File"
		acceptFilesPattern="^(?!__MACOSX)(.*).xml"
		referenceResolutionBasePathPages="/content/staffweb/us/en"
		referenceResolutionBasePathAssets="/content/dam"
		postProcessServices="[UpdatePageReferences]"
		globalConfigs="/apps/cru-content-importer/configurations/global"/>
	
	<handbook
		jcr:primaryType="nt:unstructured"
		title="Handbook"
		defaultPath="/content/staffweb/us/en"
		pageTemplate="/apps/StaffWeb/components/page/content"
		intermediateTemplate="/apps/StaffWeb/components/page/content"
		transformation="/apps/cru-content-importer/transformations/staffweb/handbook.xsl"
		rowColumnNames="{Long}0"
		columnFileName="primaryFile"
		columnMimeType=""
		pathCreationStrategy="[finalUrl:(.*)(?&lt;!\\/index)(\\/index)?\\..*:/$1]"
		pageAcceptRule=""
		factoryType="page"
		additionalMappingFile="{Boolean}true"
		additionalMappingLabel="Nodelink Mapping File"
		acceptFilesPattern="^(?!__MACOSX)(.*).xml"
		referenceResolutionBasePathPages="/content/staffweb/us/en"
		referenceResolutionBasePathAssets="/content/dam"
		postProcessServices="[UpdatePageReferences]"
		globalConfigs="/apps/cru-content-importer/configurations/global"/>

	<landingpage
		jcr:primaryType="nt:unstructured"
		title="Landing Page"
		defaultPath="/content/staffweb/us/en"
		pageTemplate="/apps/StaffWeb/components/page/content"
		intermediateTemplate="/apps/StaffWeb/components/page/content"
		transformation="/apps/cru-content-importer/transformations/staffweb/landingpage.xsl"
		rowColumnNames="{Long}0"
		columnFileName="primaryFile"
		columnMimeType=""
		pathCreationStrategy="[url:(.*)(?&lt;!\\/index)(\\/index)?\\..*:/$1]"
		pageAcceptRule="url=^(?!\\/donotmigrate)(.*)"
		factoryType="fragmentedPage"
		fragmentsColumnFileName="pageContent"
		fragmentsAcceptanceParameterPattern="^Landing_Page_Content_(.*)"
		additionalMappingFile="{Boolean}true"
		additionalMappingLabel="Nodelink Mapping File"
		acceptFilesPattern="^(?!__MACOSX)(.*).xml"
		referenceResolutionBasePathPages="/content/staffweb/us/en"
		referenceResolutionBasePathAssets="/content/dam"
		postProcessServices="[UpdatePageReferences, buildUncompletedFragmentedPages]"
		globalConfigs="/apps/cru-content-importer/configurations/global"/>
		
	<iframe
		jcr:primaryType="nt:unstructured"
		title="Iframe"
		defaultPath="/content/staffweb/us/en"
		pageTemplate="/apps/StaffWeb/components/page/content"
		intermediateTemplate="/apps/StaffWeb/components/page/content"
		transformation="/apps/cru-content-importer/transformations/staffweb/iframe.xsl"
		rowColumnNames="{Long}0"
		columnFileName="primaryFile"
		columnMimeType=""
		pathCreationStrategy="[finalUrl:(.*)(?&lt;!\\/index)(\\/index)?\\..*:/$1]"
		pageAcceptRule=""
		factoryType="page"
		additionalMappingFile="{Boolean}true"
		additionalMappingLabel="Nodelink Mapping File"
		acceptFilesPattern="^(?!__MACOSX)(.*).xml"
		referenceResolutionBasePathPages="/content/staffweb/us/en"
		referenceResolutionBasePathAssets="/content/dam"
		postProcessServices="[UpdatePageReferences]"
		globalConfigs="/apps/cru-content-importer/configurations/global"/>	

	<email
		jcr:primaryType="nt:unstructured"
		title="Email Pages"
		defaultPath="/content/staffweb/us/en"
		pageTemplate="/apps/StaffWeb/components/page/article"
		intermediateTemplate="/apps/StaffWeb/components/page/content"
		transformation="/apps/cru-content-importer/transformations/staffweb/email.xsl"
		rowColumnNames="{Long}0"
		columnFileName="primaryFile"
		columnMimeType=""
		pathCreationStrategy="[url:(.*)(?&lt;!\\/index)(\\/index)?\\..*:/$1]"
		pageAcceptRule=""
		factoryType="page"
		additionalMappingFile="{Boolean}true"
		additionalMappingLabel="Nodelink Mapping File"
		acceptFilesPattern="^(?!__MACOSX)(.*).xml"
		referenceResolutionBasePathPages="/content/staffweb/us/en"
		referenceResolutionBasePathAssets="/content/dam"
		postProcessServices="[UpdatePageReferences]"
		globalConfigs="/apps/cru-content-importer/configurations/global"/>

    <assets
        jcr:primaryType="nt:unstructured"
        title="Assets"
        defaultPath="/content/dam/staffweb"
        pageTemplate=""
        intermediateTemplate=""
        transformation="/apps/cru-content-importer/transformations/staffweb/assets.xsl"
        rowColumnNames="{Long}0"
        columnFileName="primaryFile"
        columnMimeType="primaryFile:format"
        pathCreationStrategy="[dDocType 6 30:.*:/$0,dDocAccount 6 30:.*:/$0,dCreateDate 5 20:(.{5})(\\d{4})-(\\d{2})(.*):/$2/$3,dDocName 6 30:.*:/$0,dWebExtension 6 255:.*:\\.$0]"
        pageAcceptRule=""
        factoryType="asset"
        additionalMappingFile="{Boolean}true"
        additionalMappingLabel="Nodelink Mapping File"
        acceptFilesPattern="^(?!__MACOSX)(.*)(\.(?i)(.*))$"
        globalConfigs="/apps/cru-content-importer/configurations/global"
        metadataFilePatern="(.*)(/staffweb-)(.*)"/>
        
	<video-home-article
		jcr:primaryType="nt:unstructured"
		title="Video Home Article"
		defaultPath="/content/staffweb/us/en"
		pageTemplate="/apps/StaffWeb/components/page/article"
		intermediateTemplate="/apps/StaffWeb/components/page/content"
		transformation="/apps/cru-content-importer/transformations/staffweb/video-home-article.xsl"
		rowColumnNames="{Long}0"
		columnFileName="primaryFile"
		columnMimeType=""
		pathCreationStrategy="[finalUrl:(.*)(?&lt;!\\/index)(\\/index)?\\..*:/$1]"
		pageAcceptRule=""
		factoryType="page"
		additionalMappingFile="{Boolean}true"
		additionalMappingLabel="Nodelink Mapping File"
		acceptFilesPattern="^(?!__MACOSX)(.*).xml"
		referenceResolutionBasePathPages="/content/staffweb/us/en"
		referenceResolutionBasePathAssets="/content/dam"
		postProcessServices="[UpdatePageReferences]"
		globalConfigs="/apps/cru-content-importer/configurations/global"/>

</jcr:root>
